#include<stdio.h>
#include<math.h>
int main()
{
    int num,sum,count=0,power=0,answer;
    scanf("%d",&num);
    int value=num;
    while(num>0)
    {
        count++;
        num=num/10;
        
    }
    num=value;
    while(num>0)
    {
        sum=num%10;
        power=power+(pow(sum,count));
        num=num/10;
    }
    (power==value)?printf("Yes"):printf("No");
    
    
}
