#include<stdio.h>
int main()
{
    int num;
    scanf("%d",&num);
    for(int i=num;i>=1;i--)
    {
        if(i%2!=0)
        {
            for(int j=1;j<=i;j++)
            {
                printf("%d",j);
            }
        }
        else
        {
            for(int j=i;j>=1;j--)
            {
                printf("%d",j);
            }
        }
        printf("\n");
        
    }
}
