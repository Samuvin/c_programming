#include<stdio.h>
int main()
{
    int num;
    scanf("%d",&num);
    {
        for (int row=1;row<=num;row++)
        {
            for(int col=row;col>=1;col--)
            {
                printf("%d",col);
            }
            for(int i=2;i<=row;i++)
            {
                printf("%d",i);
            }
            printf("\n");
        }
    }
}
