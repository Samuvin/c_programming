#include<stdio.h>
int main()
{
    long long int num,digit,one=0,two=0,three=0,four=0,five=0,six=0,seven=0,eight=0,nine=0,zero=0;
    scanf("%lld",&num);
    while(num>0)
    {
        digit=num%10;
        switch(digit)
        {
            case 1:
            one+=1;
            break;
            case 2:
            two+=1;
            break;
            case 3:
            three+=1;
            break;
            case 4:
            four+=1;
            break;
            case 5:
            five+=1;
            break;
            case 6:
            six+=1;
            break;
            case 7:
            seven+=1;
            break;
            case 8:
            eight+=1;
            break;
            case 9:
            nine+=1;
            break;
            case 0:
            zero+=1;
            break;
            default:
            break;
            
        }
        num=num/10;
    }
    printf("0 : %lld\n1 : %lld\n2 : %lld\n3 : %lld\n4 : %lld\n5 : %lld\n6 : %lld\n7 : %lld\n8 : %lld\n9 : %lld",zero,one,two,three,four,five,six,seven,eight,nine);
}
